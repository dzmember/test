package test;
/**
 * 测试类加载的时候静态代码块非静态代码块和静态变量，非静态变量的加载和初始化
 * */
public class Demo {
	public static int a=1;
	public int b;
	static {
		System.out.println(++a);
	}
	
	public Demo() {
		System.out.println("构造方法");
	}
	{
		System.out.println(++b);
	}
	public static void main(String[] args) throws Exception{
		Class<Demo> a = Demo.class;
		
//		Class<?> b = Class.forName("test.Demo");
		
		Demo b =  a.newInstance();
		Demo c = a.newInstance();
	}
}
