package test;

public class Demo {
	public static int a;
	public int b;
	static {
		System.out.println(a++);
	}
	{
		System.out.println(b++);
	}
	public Demo() {
		System.out.println("构造方法");
	}
	public static void main(String[] args) throws InstantiationException, IllegalAccessException {
		Class<?> a = Demo.class;
		Demo b = (Demo) a.newInstance();
	}
}
