package com.company.java.excise;

import java.text.SimpleDateFormat;

public class A {
	public static void main(String[] args) {
		SimpleDateFormat simpleFormatter1 = new SimpleDateFormat("yyyy/MM/dd");
		SimpleDateFormat simpleFormatter2= new SimpleDateFormat("yyyy-MM-dd");
		System.out.println(simpleFormatter1);
		System.out.println(simpleFormatter2);
		
	}
}